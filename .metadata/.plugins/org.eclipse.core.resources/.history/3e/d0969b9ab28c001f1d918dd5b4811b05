package p1.max;

public class SecondMin {

	public static void main(String[] args) {
		int temp;
		int[] a = { 1, 2, 4, 5, 6, 7, 8, 9 };
		for (int i = 0; i < a.length; i++) {
			for (int j = 1; j < a.length; j++) {
				if (a[i] < a[j]) {
					temp = a[j];
					a[j] = a[i];
					a[i] = temp;

				}
			}

			System.out.println(a[2]);
		}

	}

}
