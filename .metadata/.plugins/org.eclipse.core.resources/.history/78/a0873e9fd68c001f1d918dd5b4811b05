package duplicateArray;

public class DuplicateNumber {
	public static void main(String[] args) {
		int a[] = { 2, 4, 5, 6, 7, 8, 8, 3 };
		int count = 0;

		for (int i = 0; i < a.length; i++) {
			for (int j = i + 1; j < a.length; j++) {
				if (a[i] == a[j]) {
					count++;
					System.out.println(a[j]);

				}


			}

		}

	}

}
