package sumarray;

public class OddEvenArray {
	public static void main(String[] args) {
		int a[] = { 7, 2, 9, 4, 5, 8 };
		for (int i = 0; i < a.length; i++) {
			if (a[i] % 2 == 0) {
				System.out.println("Even" + a[i] + "");

			} else if (!(a[i] % 2 == 0)) {

				System.out.println("odd " + a[i]);

			}

		}
	}

}
