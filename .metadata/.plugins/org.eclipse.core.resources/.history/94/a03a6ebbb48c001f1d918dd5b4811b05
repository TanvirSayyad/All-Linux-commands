package p1.max;

public class SecondMax {
	public static void main(String[] args) {
		int[] a = { 1, 2, 3, 4, 5, 6, 7 };
		int temp;
		for (int i = 0; i < a.length - 1; i++) {
			for (int j = i + 1; j < a.length; j++) {
				if (a[i] < a[j]) {

					temp = a[j];
					a[j] = a[i];
					a[i] = temp;

				}
			}
		}

		System.out.println("second max is 6====" + a[1]);
	}

}
