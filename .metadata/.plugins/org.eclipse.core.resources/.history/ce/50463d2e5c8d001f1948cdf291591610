package missingNumber;

public class MissNumberIndex {
	public static void main(String[] args) {
		int a[] = { 2, 4, 8, 10 };
		int sum = 0;
		int sum2 = 0;
		for (int i = 0; i < a.length; i++) {

			sum = sum + a[i];
		}
		System.out.println(sum);

		for (int i = a[0]; i <= 4; i++) {
			sum2 = sum2 +i ; //

		}
		System.out.println("sum2 is" + sum2);

		System.out.println(sum - sum2);
	}
}
